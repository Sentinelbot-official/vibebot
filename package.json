{
  "name": "vibe-bot",
  "version": "2.1.0",
  "description": "A feature-rich Discord bot with 200+ commands, advanced moderation, economy, leveling, social features, mini-games, and comprehensive auto-mod systems",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node --trace-warnings index.js",
    "dev": "node --watch --trace-warnings index.js",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "db:backup": "node scripts/backup.js",
    "health": "node scripts/health-check.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discord-bot",
    "moderation",
    "economy",
    "leveling",
    "automod"
  ],
  "author": "Airis",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sentinelbot-official/vibebot.git"
  },
  "bugs": {
    "url": "https://github.com/Sentinelbot-official/vibebot/issues"
  },
  "homepage": "https://github.com/Sentinelbot-official/vibebot#readme",
  "dependencies": {
    "axios": "^1.6.5",
    "better-sqlite3": "^11.8.1",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "ms": "^2.1.3"
  },
  "optionalDependencies": {
    "@napi-rs/canvas": "^0.1.44"
  },
  "devDependencies": {
    "prettier": "^3.7.4"
  }
}
